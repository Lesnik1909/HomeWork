d[3] = "\u2557"; // правый верхний угол
d[4] = "\u255A"; // левый нижний угол
d[5] = "\u255D"; // правый нижний угол
d[7] = "\u2563"; // правое перекрестье
d[8] = "\u2566"; // верхнее перекрестье
d[9] = "\u2569"; // нижнее перекрестье
d[10] = "\u256C"; // центральное перекрестье
d[11] = "\u0020"; // пустая клетка
